{
  "name": "vue_app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:dev": "vue-cli-service serve",
    "start:test": "vue-cli-service serve --mode test",
    "build": "vue-cli-service build && node scripts/compress.js",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "tep": "node scripts/template",
    "com": "node scripts/component"
  },
  "dependencies": {
    "@types/lodash": "^4.14.138",
    "archiver": "^3.1.1",
    "axios": "^0.19.0",
    "core-js": "^2.6.5",
    "fastclick": "^1.0.6",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.21",
    "mddir": "^1.1.1",
    "vant": "^2.1.8",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/chai": "^4.1.0",
    "@types/fastclick": "^1.0.28",
    "@types/js-cookie": "^2.2.2",
    "@types/mocha": "^5.2.4",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-e2e-cypress": "^3.11.0",
    "@vue/cli-plugin-typescript": "^3.11.0",
    "@vue/cli-plugin-unit-mocha": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "chai": "^4.1.2",
    "chalk": "^2.4.2",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.1.0",
    "tslint": "^5.19.0",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.4.3",
    "vue-template-compiler": "^2.6.10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "tslint --fix",
      "git add"
    ]
  }
